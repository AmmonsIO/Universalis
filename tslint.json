{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": [ false ],
        "interface-name": [ false ],
        "no-angle-bracket-type-assertion": [ false ],
        "no-console": {
            "severity": "warning"
        },
        "no-string-literal": [ false ],
        "no-var-keyword": [ false ],
        "prefer-const": [ false ],
        "radix": [ false ],
        "trailing-comma": [ false ]
    },
    "rulesDirectory": []
}
