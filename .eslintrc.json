{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2019
    },
    "rules": {
        "camelcase": "off",
        "indent": "off",
        "linebreak-style": "off",
        "max-len": "off",
        "no-console": "off",
        "no-unused-vars": "off",
        "no-var": "off",
        "prefer-const": "off",
        "quotes": ["off", "double"]
    }
}
